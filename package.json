  "devDependencies": {
    "concurrently": "^6.2.1"
  },
  "dependencies": {
    "@apollo/client": "^3.4.5",
    "apollo-server-express": "^2.25.2",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
